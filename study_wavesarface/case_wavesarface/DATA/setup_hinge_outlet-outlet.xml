double omega = 2.0*pi/T;

double theta_dot = (S/h) * omega * cos(omega*t);

mesh_velocity[0] = -(y - y_h) * theta_dot;
mesh_velocity[1] = 0.0;
mesh_velocity[2] = 0.0;</formula>
